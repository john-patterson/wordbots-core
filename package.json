{
  "name": "wordbots",
  "version": "0.8.1-alpha",
  "description": "A card-and-board game that showcases card game semantic parsing",
  "license": "MIT",
  "browserslist": [
    "last 4 versions",
    "chrome >= 48"
  ],
  "scripts": {
    "slate": "rm -rf node_modules && npm install",
    "clean": "rm -rf dist",
    "start": "set NODE_ENV=development && node src/server/index.js --colors --profile",
    "start-prod": "set NODE_ENV=production && node src/server/index.js --progress --colors --profile",
    "build": "npm run clean && set NODE_ENV=production && webpack -p --progress --colors --profile",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "heroku-postbuild": "npm run clean && set NODE_ENV=production && webpack -p --progress --colors --profile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wordbots/wordbots-core.git"
  },
